<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<configSections>
		<section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
		<section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
		<!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
	</configSections>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
	</startup>
	<connectionStrings>
		<add name="CLOContext" connectionString="Data Source=UATDWHSQL;Uid=YodaUser;Database=Yoda;Pwd=*****;MultipleActiveResultSets=True" providerName="System.Data.SqlClient" />
	</connectionStrings>
	<appSettings>
		<add key="ServiceBaseUrl" value="http://localhost:9000/clo" />
		<add key="ReCycleRestService" value="true" />
		<add key="SupportToEmailIds" value="developers@yorkcapital.com" />
		<add key="ReplyToName" value="CLO Support" />
		<add key="Environment" value="UAT" />
		<add key="ReplyToEmail" value="developers@yorkcapital.com" />
		<add key="CLOSupportName" value="CLO Support" />
		<add key="CLOSupportEmail" value="closupport@yorkcapital.com" />
		<add key="PriceMoverURL" value="http://localhost/yorknet/clo/home/topbottompricemovers" />
		<add key="PriceMoverToEmailIds" value="osantiago@yorkcapital.com" />
		<add key="PriceMoverCCEmailIds" value="osantiago@yorkcapital.com" />
		<add key="PriceMoverSubject" value="Price Movers as of {date}" />

		<add key="MoodysAdjustedCfrMismatchEmailIds" value="ssingh@yorkcapital.com" />
		<add key="MoodysAdjustedCfrMismatchURL" value="http://localhost/yorknet/clo/home/mismatchdata" />
		<add key="MoodysAdjustedCfrMismatchSubject" value="Moody's Adjusted CFR Mismatch as of {date}" />
		<add key="MoodysAdjustedCfrMismatchCCEmailIds" value="ssingh@yorkcapital.com" />


		<add key="RatingChangeUrl" value="http://localhost/yorknet/clo/home/ratingchanges?startDateId=20191029&amp;endDateId=20191030" />
		<add key="RatingChangeToEmailIds" value="osantiago@yorkcapital.com" />
		<add key="RatingChangeCCEmailIds" value="osantiago@yorkcapital.com" />
		<add key="RatingChangeSubject" value="CLO Rating Changes on {date}" />
		<add key="FundDailySnapshotURL" value="http://localhost/Yorknet/clo/home/fundstalestatus" />
		<add key="FundDailySnapshotSubject" value="WSO/CLO status - as of {date}" />
		<add key="FundDailySnapshotToEmailIds" value="developers@yorkcapital.com" />
		<add key="FundDailySnapshotCCEmailIds" value="developers@yorkcapital.com" />
		<add key="SourceDirectory" value="C:\CLO" />
		<add key="ArchiveDirectory" value="C:\CLO\Archive" />
		<add key="ClientSettingsProvider.ServiceUri" value="" />
		<add key="MoodyBasedWarfCalculationFundIds" value="4,10,2,1,3" />
	</appSettings>

	<runtime>
		<assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
			<dependentAssembly>
				<assemblyIdentity name="FSharp.Core" publicKeyToken="b03f5f7f11d50a3a" />
				<bindingRedirect oldVersion="0.0.0.0-4.4.3.0" newVersion="4.4.3.0" />
			</dependentAssembly>
		</assemblyBinding>
	</runtime>

	<system.net>
		<mailSettings>
			<smtp>
				<network host="webmail.yorkcapital.com" port="25" />
			</smtp>
		</mailSettings>
	</system.net>
	<log4net>
		<appender name="LogFileAppender" type="log4net.Appender.RollingFileAppender">
			<param name="File" value="Logs\" />
			<lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
			<appendToFile value="true" />
			<rollingStyle value="Date" />
			<maxSizeRollBackups value="2" />
			<datePattern value="yyyyMMdd'.log'" />
			<maximumFileSize value="10MB" />
			<staticLogFileName value="false" />
			<layout type="log4net.Layout.PatternLayout">
				<param name="ConversionPattern" value="%d [%t] %-5p %c %m%n" />
			</layout>
		</appender>
		<root>
			<level value="ALL" />
			<appender-ref ref="LogFileAppender" />
		</root>
		<logger name="test">
			<level value="DEBUG">
			</level>
		</logger>
	</log4net>
	<runtime>
		<assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
			<dependentAssembly>
				<assemblyIdentity name="FSharp.Core" publicKeyToken="b03f5f7f11d50a3a" culture="neutral" />
				<bindingRedirect oldVersion="0.0.0.0-4.4.0.0" newVersion="4.3.1.0" />
			</dependentAssembly>
		</assemblyBinding>
	</runtime>
	<entityFramework>
		<defaultConnectionFactory type="System.Data.Entity.Infrastructure.SqlConnectionFactory, EntityFramework" />
		<providers>
			<provider invariantName="System.Data.SqlClient" type="System.Data.Entity.SqlServer.SqlProviderServices, EntityFramework.SqlServer" />
		</providers>
	</entityFramework>
	<system.web>
		<membership defaultProvider="ClientAuthenticationMembershipProvider">
			<providers>
				<add name="ClientAuthenticationMembershipProvider" type="System.Web.ClientServices.Providers.ClientFormsAuthenticationMembershipProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri="" />
			</providers>
		</membership>
		<roleManager defaultProvider="ClientRoleProvider" enabled="true">
			<providers>
				<add name="ClientRoleProvider" type="System.Web.ClientServices.Providers.ClientRoleProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri="" cacheTimeout="86400" />
			</providers>
		</roleManager>
	</system.web>
</configuration>
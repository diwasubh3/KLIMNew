
@model List<List<YCM.CLO.DataAccess.Models.DataExceptionObject>>

@{
    Layout = "../Shared/_BlankLayout.cshtml";
}

@if (Model.Count > 0)
{
    for (int i = 0; i < Model.Count; i++)
    {
        <div style="padding:20px">
            @if (i == 0)
            {
                <h4>
                    <u>S & P Industry</u>
                </h4>
            }
            @if (i == 1)
            {
                <h4>
                    <u>Frequency</u>
                </h4>
            }
